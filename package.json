{
  "name": "native-to-big",
  "version": "0.0.1",
  "description": "Convert native math expressions to Big types from the big.js library",
  "main": "build/bin/main.js",
  "bin": {
    "n2b": "build/bin/main.js"
  },
  "scripts": {
    "start": "ts-node bin/main.ts",
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf build",
    "test": "jest",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [
    "native to big.js",
    "convert to big.js",
    "big.js converter",
    "big.js generator"
  ],
  "author": "Sleeyax",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sleeyax/native-to-big"
  },
  "dependencies": {
    "commander": "^9.0.0",
    "ts-morph": "^13.0.3"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.3",
    "@types/jest": "^27.4.1",
    "big.js": "^6.1.1",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
